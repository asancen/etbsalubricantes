    <title>
        ETBSA Lubricantes - @ViewBag.Title
    </title>

    <meta name="author" content="apptitude">

    <!-- Mobile Specific Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <!-- Bootstrap  -->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/stylesheets/bootstrap.css")">

    <!-- Theme Style -->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/stylesheets/style.css")">

    <!-- Responsive -->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/stylesheets/responsive.css")">

    <!-- REVOLUTION LAYERS STYLES -->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/revolution/css/layers.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/revolution/css/settings.css")">

    <!-- Colors -->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/stylesheets/colors/color1.css")" id="colors">

    <!-- Animation Style -->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/stylesheets/animate.css")">

    <!-- Favicon and touch icons  -->
    <link href="@Url.Content("~/icon/apple-touch-icon-48-precomposed.png")" rel="apple-touch-icon-precomposed" sizes="48x48">
    <link href="@Url.Content("~/icon/apple-touch-icon-32-precomposed.png")" rel="apple-touch-icon-precomposed">
    <link href="@Url.Content("~/icon/favicon.ico")" rel="shortcut icon">

    

    <!--[if lt IE 9]>
        <script src="@Url.Content("~/javascript/html5shiv.js")"></script>
        <script src="@Url.Content("~/javascript/respond.min.js")"></script>
    <![endif]-->

    <style type="text/css">
        /* Overlay JS*/

        #overlay {
            display: none;
            position: fixed;
            background: #000;
            z-index: 100000;
            width: 100%;
            height: 100%;
        }

        #img-load {
            position: absolute;
            z-index: 100001;
        }
        .btn-warning{
            background-color: #e6e6e6;
            border-color: lightgray;
            width: 100%;
            text-align: left;
        }
        .btn-warning:hover{
            background-color: lightblue;
            border-color: black;
        }
    </style>
</head>
<body class="header_sticky onepage">
    <div id="overlay">
        <span id="img-load"><img src="@Url.Content("~/images/ajax-loader.gif")" /></span>
    </div>
    <!-- Preloader -->
    <section class="loading-overlay">
        <div class="Loading-Page">
            <h2 class="loader">Loading</h2>
        </div>
    </section>
    <!-- Boxed -->
    <div class="boxed">
        <div class="fl-wrap-header parallax parallax1">
            <div class="overlay"></div>
            <!-- Header -->
            <header id="header" class="header clearfix">
                <div class="container">
                    <div class="row">
                        <div class="header-wrap clearfix">
                            <div class="col-md-4">
                                <div id="logo" class="logo">
                                    <a href="@Url.Content("~/")" rel="home">
                                        <img src="@Url.Content("~/images/logo2.png")" alt="image">
                                    </a>
                                </div><!-- /.logo -->
                                <div class="btn-menu">
                                    <span></span>
                                </div><!-- //mobile menu button -->
                            </div>
                            <div class="col-md-8">
                                <div class="nav-wrap">
                                    <nav id="mainnav" class="mainnav">
                                        <ul class="menu">
                                            <li class="">
                                                <a href="@Url.Content("~/")">Inicio</a>
                                            </li>
                                            <li><a href="#about">¿Quiénes somos?</a></li>
                                            <li>
                                                <a href="#portfolio">Productos</a>
                                                <ul class="submenu right-sub-menu">
                                                    <li>
                                                        <a href="@Url.Content("~/Productos/TransporteComercial")">Transporte comercial </a>
                                                    </li>
                                                    <li>
                                                        <a href="@Url.Content("~/Productos")">Vehículos de pasajeros</a>
                                                    </li>
                                                    <li>
                                                        <a href="@Url.Content("~/Productos")">Industrial</a>
                                                    </li>
                                                    <li>
                                                        <a href="@Url.Content("~/Productos")">Agrícola</a>
                                                    </li>
                                                </ul><!-- /.submenu -->
                                            </li>
                                            <li><a href="#contact">Contacto</a></li>
                                        </ul><!-- /.menu -->
                                    </nav><!-- /.mainnav -->
                                    @*<div class="top-search">
                                                <form role="search" id="searchform-all" method="get" class="search-form" action="#">
                                                    <label>
                                                        <input type="search" id="s" class=" sss search-field" placeholder="Search …" value="" name="s">
                                                    </label>
                                                    <input type="submit" class="search-submit" value="" id="searchsubmit">
                                                </form>
                                            </div><!-- /top-search -->

                                        <ul class="menu menu-extra">
                                            <li class="show-search">
                                                <a href="#"><i class="fa fa-search"></i></a>
                                            </li>
                                        </ul>*@
                                </div><!-- /.nav-wrap -->

                            </div>
                        </div><!-- /.header-inner -->
                    </div><!-- /.row -->
                </div>
            </header><!-- /.header -->
            @RenderSection("banner", required: false)
        </div>
        @RenderBody()
        <!-- Footer -->
        <footer class="footer">
            <div class="footer-widgets">
                <div class="container">
                    <div class="row">
                        <div class="col-md-3 col-sm-6">
                            <div class="widget widget_text widget_info">
                                <img src="@Url.Content("~/images/logo2.png")" alt="images">
                            </div><!-- /.widget -->
                        </div><!-- /.col-md-3 -->

                        <div class="col-md-3 col-sm-6">
                            <div id="nav_menu-2" class="widget widget_nav_menu">
                                <h2 class="widget-title">Categorias</h2>
                                <div class="menu-footer-container">
                                    <ul id="menu-footer" class="menu">
                                        <li class="menu-item"><a href="@Url.Content("~/Productos/TransporteComercial")">Transporte</a></li>
                                        <li class="menu-item"><a href="#">Vehículos de pasajeros</a></li>
                                        <li class="menu-item"><a href="#">Industrial</a></li>
                                        <li class="menu-item"><a href="#">Agrícola</a></li>
                                    </ul>
                                </div>
                            </div>
                        </div><!-- /.col-md-3 -->

                        <div class="col-md-3 col-sm-6">
                            <div class="widget widget_text">
                                <h2 class="widget-title">Contacto</h2>
                                <p>
                                    Carr. Panamericana Celaya-Salamanca Km 61, CP 38120 Celaya, Gto.
                                </p>

                                <p>Teléfonos: 01 (461) 61 423 23 al 25</p>
                            </div>
                        </div><!-- /.col-md-3 -->
                    </div><!-- /.row -->
                </div><!-- /.container -->
            </div><!-- /.footer-widgets -->

        </footer>

        <!-- Bottom -->
        <div class="bottom">
            <div class="container">
                <div class="row">
                    <div class="col-md-6">
                        <div class="copyright">
                            <p>
                                © Desarrollado por <a href="http://apptitude.mx/" target="_blank">Apptitude</a>, @DateTime.Now.Year.ToString()
                            </p>
                        </div>
                    </div><!-- /.col-md-6 -->

                    <div class="col-md-6">
                        <div class="social-links">
                            <a href="https://www.facebook.com/ChevronBajio/" target="_blank"><i class="fa fa-facebook"></i></a>
                            @*<a class="twitter" href="#"><i class="fa fa-twitter"></i></a>
                                <a class="google" href="#"><i class="fa fa-google-plus" aria-hidden="true"></i></a>
                                <a class="youtube" href="#"><i class="fa fa-youtube" aria-hidden="true"></i></a>*@
                        </div>
                    </div><!-- /.row -->
                </div>
            </div><!-- /.container -->
        </div>

        <!-- Go Top -->
        <a class="go-top">
            <i class="fa fa-angle-up"></i>
        </a>

    </div>

    <!-- Javascript -->
    <script type="text/javascript" src="@Url.Content("~/javascript/jquery.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/javascript/bootstrap.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/javascript/jquery.easing.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/javascript/imagesloaded.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/javascript/jquery.isotope.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/javascript/jquery-waypoints.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/javascript/jquery.magnific-popup.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/javascript/jquery.cookie.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/javascript/parallax.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/javascript/owl.carousel.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/javascript/jquery-validate.js")"></script>

    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAHtWltCIolOgzpMEfi_GVZZHCkrXH10SM&region=GB"></script>
    @*<script type="text/javascript" src="@Url.Content("~/javascript/gmap3.min.js")"></script>*@
    <script type="text/javascript" src="@Url.Content("~/javascript/main.js")"></script>

    <!-- Revolution Slider -->
    <script type="text/javascript" src="@Url.Content("~/revolution/js/jquery.themepunch.tools.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/revolution/js/jquery.themepunch.revolution.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/revolution/js/slider.js")"></script>

    <!-- SLIDER REVOLUTION 5.0 EXTENSIONS  (Load Extensions only on Local File Systems !  The following part can be removed on Server for On Demand Loading) -->
