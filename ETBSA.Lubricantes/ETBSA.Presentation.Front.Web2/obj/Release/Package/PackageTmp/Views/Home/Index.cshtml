<!-- Map -->
@using (Ajax.BeginForm("Index", new AjaxOptions { HttpMethod = "POST", UpdateTargetId = "correcto", OnComplete = "CompleteInsert", OnBegin = "BeginInsert", OnFailure = "FailureInsert", OnSuccess = "SuccessInsert", InsertionMode = InsertionMode.Replace }))
{
    @Html.AntiForgeryToken()
    <div class="flat-row map" id="contact">
        <div id="contactform" class="flat-contact-form  inner-map style2 bg-dark height-small">

            <div class="field clearfix">
                <div class="wrap-type-input">
                    <div class="input-wrap phone">
                        @*<input type="text" value="" tabindex="1" placeholder="Nombre" name="name" id="name" required>*@
                        @Html.TextBoxFor(m => m.Nombre, new { placeholder = "Nombre", tabindex = "1" })
                        @Html.ValidationMessageFor(m => m.Nombre)
                    </div>
                </div>
                <div class="wrap-type-input">
                    <div class="input-wrap name">
                        @*<input type="text" value="" tabindex="1" placeholder="Nombre" name="name" id="name" required>*@
                        @Html.TextBoxFor(m => m.Telefono, new { placeholder = "Teléfono", tabindex = "2" })
                        @Html.ValidationMessageFor(m => m.Telefono)
                    </div>
                    <div class="input-wrap email">
                        @*<input type="email" value="" tabindex="2" placeholder="Correo" name="email" id="email" required>*@
                        @Html.TextBoxFor(m => m.Correo, new { placeholder = "Correo", tabindex = "3" })
                        @Html.ValidationMessageFor(m => m.Correo)
                    </div>
                </div>
                <div class="textarea-wrap">
                    @*<textarea class="type-input" tabindex="3" placeholder="Mensaje" name="message" id="message" required></textarea>*@
                    @Html.TextAreaFor(m => m.Mensaje, new { placeholder = "Mensaje", tabindex = "4", @class = "type-input" })
                </div>
            </div>
            <div class="submit-wrap">
                <input type="submit" class="flat-button bg-theme" value="Enviar mensaje" />
            </div>
            <!-- /.comment-form -->
        </div>
        <div id="flat-map">
        </div>
    </div><!-- /.flat-row -->
}

